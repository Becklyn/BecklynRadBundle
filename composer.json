{
    "name": "becklyn/rad-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides basic components",
    "homepage": "https://github.com/Becklyn/RadBundle",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "beberlei/doctrineextensions": "^1.2",
        "becklyn/rad-bundles": "^1.0.1",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "psr/log": "^1.1",
        "scienta/doctrine-json-functions": "^4.1",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/form": "^4.4 || ^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/property-access": "^4.4 || ^5.0",
        "symfony/translation-contracts": "^1.1 || ^2.0",
        "symfony/validator": "^5.0",
        "twig/twig": "^2.11 || ^3.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/twig-bridge": "^5.0"
    },
    "suggest": {
        "symfony/twig-bridge": "To use the default form theme."
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-next": "7.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\RadBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\RadBundle\\": "tests/"
        }
    }
}
