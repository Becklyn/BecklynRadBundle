{
    "name": "becklyn/rad-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides basic components",
    "homepage": "https://github.com/Becklyn/RadBundle",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "symfony/config": "^4.3|^5.0",
        "symfony/dependency-injection": "^4.3|^5.0",
        "symfony/form": "^4.3|^5.0",
        "symfony/http-kernel": "^4.3|^5.0",
        "symfony/translation-contracts": "^1.1|^2.0",
        "twig/twig": "^2.11|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "roave/security-advisories": "dev-master",
        "symfony/twig-bridge": "^4.3|^5.0"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "To use the DoctrineModel.",
        "doctrine/orm": "To use the DoctrineModel, the SortableHelper and the entity traits.",
        "symfony/twig-bridge": "To use the default form theme."
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\RadBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\RadBundle\\": "tests/"
        }
    }
}
